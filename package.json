{
    "name": "@jitesoft/audit-for-gitlab",
    "version": "1.0.0",
    "description": "Tiny converter to convert npm audit report into gitlab-ci vulnerability report format.",
    "main": "run.js",
    "bin": "run.js",
    "preferGlobal": true,
    "scripts": {
        "test": "eslint index.js",
        "build:prod": "babel index.js -o bin.js"
    },
    "keywords": [
        "CI",
        "gitlab",
        "jitesoft",
        "convert",
        "audit",
        "dependency-scan",
        "dependency",
        "depscan",
        "vulnerabilities",
        "vuln",
        "report"
    ],
    "dependencies": {
        "@babel/runtime-corejs3": "^7.10.4"
    },
    "devDependencies": {
        "eslint": "^7.4.0",
        "babel-eslint": "^10.1.0",
        "@jitesoft/eslint-config": "^2.2.0",
        "core-js": "^3.6.5",
        "@babel/cli": "^7.10.4",
        "@babel/core": "^7.10.4",
        "@jitesoft/babel-preset-main": "^2.3.2"
    },
    "eslintConfig": {
        "extends": "@jitesoft"
    },
    "babel": {
        "presets": [
            [
                "@jitesoft/main",
                {
                    "mode": "node"
                }
            ]
        ]
    },
    "author": {
        "email": "johannes@jitesoft.com",
        "ur": "https://jitesoft.com",
        "name": "Johannes Tegn√©r"
    },
    "license": "MIT"
}
